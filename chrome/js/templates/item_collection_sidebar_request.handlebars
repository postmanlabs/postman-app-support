
    <li id="sidebar-request-{{id}}" class="sidebar-collection-request sidebar-request clearfix">
        <div class="request-actions">
            <a href="javascript:void(0);" data-id="{{id}}" class="request-actions-edit">
                <img src="img/edit.png" style="vertical-align: middle;"/>
            </a>
            <a href="javascript:void(0);" data-id="{{id}}" class="request-actions-delete">
                <img src="img/delete.png" style="vertical-align: middle;"/>
            </a>
        </div>
        <div class="request request-actions-load" data-id="{{id}}">
            <a href="javascript:void(0);"><span class="label label-method-{{method}}">{{method}}</span>
                <span class="request-name">
                {{html name}}
                </span>
            </a>
        </div>
    </li>